{
  "name": "kurssiesitieto",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently \"nodemon backend/index.js\" \"vite ./frontend\"",
    "build": "vite build ./frontend --outDir ../dist",
    "prod": "NODE_ENV=production node backend/index.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 5",
    "test": "jest",
    "test:frontend": "jest frontend/ --config jest.config.frontend.js",
    "test:backend": "jest backend/ --config jest.config.backend.js",
    "coverage:frontend": "jest frontend/ --coverage --config jest.config.frontend.js --collectCoverageFrom=\"./frontend/src/**\"",
    "coverage:backend": "jest backend/ --coverage --config jest.config.backend.js --collectCoverageFrom=\"./backend/**\"",
    "test-coverage": "npm run test:frontend && codecov && npm run test:backend && codecov"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.11",
    "@mui/material": "^5.15.7",
    "axios": "^1.6.5",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cypress": "^13.7.0",
    "dagre-reactjs": "^1.0.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "reactflow": "^11.10.1",
    "util.promisify": "^1.1.2",
    "winston": "^3.12.0",
    "winston-console-format": "^1.0.8"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.16.7",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.55.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "vite": "^5.0.8"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
